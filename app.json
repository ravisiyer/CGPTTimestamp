{
  "expo": {
    "name": "Dev Timestamp Tracker",
    "slug": "dev-timestamp-tracker",
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/time-stamp-icon-wb.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "experiments": {
      "baseUrl": "/CGPTTimestamp"
    },
    "splash": {
      "image": "./assets/time-stamp-icon-wb.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ravisiyer.devtimestamptracker",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/time-stamp-icon-wb.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ravisiyer.devtimestamptracker"
    },
    "web": {
      "favicon": "./assets/time-stamp-icon-wb.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "b10c5153-3cb6-440b-8610-bbffce683b94"
      }
    },
    "owner": "ravisiyer"
  }
}
